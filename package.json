{
  "name": "bestofjs-webui",
  "version": "0.4.5",
  "description": "bestof.js.org front-end web application",
  "main": "",
  "engines": {
    "node": ">=5.10"
  },
  "scripts": {
    "lint": "eslint src --ext .js --ext .jsx",
    "test": "cross-env NODE_ENV=test babel-tape-runner test/**/*Spec.js | faucet",
    "test-debug": "babel-tape-runner test/**/ProjectListSpec.js",
    "clean": "rimraf www/index.html && rimraf www/build",
    "build": "npm run build-html && npm run build-js",
    "start": "cross-env NODE_ENV=development node config/devserver.js",
    "build-js": "cross-env NODE_ENV=production webpack -p --config config/webpack.production.config.js --progress",
    "gh-pages": "node scripts/deploy-gh-pages/",
    "firebase": "firebase deploy --firebase bestofjs --public ./www",
    "surge": "surge --domain bestofjs.surge.sh --project ./www/",
    "deploy-all": "npm run build && npm run gh-pages",
    "daily": "npm run build-html && npm run build-html-hof && npm run deploy-html",
    "build-html": "babel-node scripts/build/",
    "build-html-hof": "babel-node scripts/build/build-hof.js",
    "deploy-html": "babel-node scripts/deploy-html/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/michaelrambeau/bestofjs-webui.git"
  },
  "keywords": [
    "react",
    "redux",
    "webpack"
  ],
  "author": "Michael Rambeau",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/michaelrambeau/bestofjs-webui/issues"
  },
  "homepage": "http://bestof.js.org",
  "dependencies": {
    "autoprefixer": "^6.3.3",
    "babel-cli": "6.5.1",
    "babel-core": "6.5.2",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.5.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-optimize": "^1.0.1",
    "cross-env": "^1.0.7",
    "css-loader": "0.23.1",
    "dotenv": "^2.0.0",
    "es6-promise": "^3.0.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "fs-extra": "^0.26.5",
    "html-minifier": "^1.0.0",
    "html-webpack-plugin": "2.16.0",
    "imports-loader": "^0.6.5",
    "isomorphic-fetch": "^2.2.1",
    "json-loader": "^0.5.4",
    "lodash": "^3.8.0",
    "marked": "^0.3.5",
    "node-fetch": "^1.3.3",
    "notie": "^2.1.0",
    "numeral": "^1.5.3",
    "postcss-loader": "^0.8.1",
    "react": "15.0.1",
    "react-addons-shallow-compare": "^15.0.1",
    "react-dom": "15.0.1",
    "react-redux": "4.4.5",
    "react-router": "2.3.0",
    "react-router-scroll": "0.2.0",
    "react-select": "1.0.0-beta13",
    "react-virtualized": "^7.3.2",
    "redux": "^3.0.4",
    "redux-form": "5.1.3",
    "redux-logger": "^2.0.2",
    "redux-thunk": "^1.0.0",
    "slideout": "^0.1.11",
    "style-loader": "0.13.0",
    "stylus-loader": "1.5.1",
    "url-loader": "^0.5.7",
    "vague-time": "^1.3.0",
    "webpack": "1.12.13"
  },
  "devDependencies": {
    "babel-register": "6.5.2",
    "babel-tape-runner": "2.0.1",
    "enzyme": "2.2.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.1.1",
    "eslint-plugin-react": "^3.11.3",
    "faucet": "0.0.1",
    "gh-pages": "0.10.0",
    "jsdom": "^7.0.2",
    "react-addons-perf": "^15.0.1",
    "react-addons-test-utils": "15.0.1",
    "react-hot-loader": "1.3.0",
    "sinon": "^1.17.3",
    "tape": "^4.2.2",
    "webpack-dev-server": "1.14.1"
  }
}
